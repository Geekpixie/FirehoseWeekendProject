<div class="span6 offset3 text-center">
	<h1>Add New Picture!<h1>
		<div class="boxy text-center">
			<%= simple_form_for @pic do |f| %>
				<%= f.input :hour %>
				<%= f.input :emotion %>
				<%= f.input :learned %>
				<%= submit_tag "Create", :class => 'btn-warning btn btn-large' %>
			<% end %>
		</div>
</div>